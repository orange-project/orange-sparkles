- if model
  %td.move
    &nbsp;
  - for prop in props
    - if(prop[:relationship])
      %td= [:belongs, :has_one].include?(prop[:type]) ? model.__send__(prop[:name]).scaffold_name : ""
    - else
      %td= model.attribute_get(prop[:name])
  %td.actions
    %div.page-listing-actions
      %a.button.grey-button.preview-button{:href => route_to(model_name, model.id, "edit")}
        Edit
      = delete_button(route_to(model_name, model.id, "delete"))